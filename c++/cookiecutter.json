{
  "directory_name": "HelloWorld",
  "cmake_project_name": "HelloWorld",
  "cmake_version": "0.1.0",
  "cmake_description": "\"Simple hello world program\"",
  "projectile_configure": "\"cmake -S . --preset=gcc-ninja-debug-cxx\"",
